{
  "name": "babyai",
  "private": true,
  "version": "0.1.0",
  "description": "BabyAI monorepo",
  "scripts": {
    "prisma:validate": "prisma validate --schema packages/schema/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema packages/schema/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema packages/schema/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema packages/schema/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema packages/schema/prisma/schema.prisma",
    "prisma:migrate:status": "prisma migrate status --schema packages/schema/prisma/schema.prisma",
    "prisma:migrate:baseline:init": "prisma migrate resolve --applied 20260220224000_init --schema packages/schema/prisma/schema.prisma"
  },
  "devDependencies": {
    "prisma": "6.16.2"
  },
  "dependencies": {
    "@prisma/client": "6.16.2"
  }
}
